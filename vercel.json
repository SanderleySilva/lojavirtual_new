{
  "version": 2,
  "builds": [
    {
      "src": "lojaproject/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.11.3" }
    },
    {
      "src": "manage.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "lojaproject/wsgi.py"
    }
  ]
}
